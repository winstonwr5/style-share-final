<div class="center-div">

    <h2>Enter Your Prompt Pic:</h2>

    <div class="sketch-form">
    <%= form_for :resolution, url: pages_show_path do |form| %>
    <%= form.file_field :image %>
    <%# <%= render :partial => 'prompts/form' %>
    <% end %>
    </div>


    <h2>Your Canvas</h2>

    <%= form_for :resolution, url: pages_show_path do |f| %>
    <%= f.select :res_string, options_for_select([["1024x768"],["800x600"],["640x480"]]) %>
    <%= f.submit "Change" %>
    <% end %>

    <p id="canvas-size"></p>
    <div id="p5canvas">
    </div>
    <p>
        <button id="saveButton">save sketch</button>
        <button id="clearButton">clear</button>
    </p>
</div>

<%= render 'user_canvas' %>

